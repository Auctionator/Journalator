<Ui xmlns="http://www.blizzard.com/wow/ui/">
  <Frame
    name="JournalatorInvoiceDisplayTemplate"
    virtual="true"
    mixin="JournalatorDisplayTabMixin"
  >
    <Scripts>
      <OnLoad method="OnLoad" />
    </Scripts>

    <Frames>
      <Frame parentKey="ResultsListing" inherits="AuctionatorResultsListingTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT"/>
          <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="-20" y="40"/>
        </Anchors>
      </Frame>

      <Frame parentKey="ResultsListingInset" inherits="AuctionatorInsetDarkTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent.ResultsListing" relativePoint="TOPLEFT" x="-5" y="-24"/>
          <Anchor point="BOTTOMRIGHT" relativeKey="$parent.ResultsListing" relativePoint="BOTTOMRIGHT" y="-2"/>
        </Anchors>
      </Frame>

      <Frame parentKey="DataProvider" mixin="JournalatorInvoicesDataProviderMixin">
        <Scripts>
          <OnLoad method="OnLoad"/>
          <OnShow method="OnShow"/>
          <OnUpdate method="OnUpdate"/>
        </Scripts>
      </Frame>
    </Frames>

    <Layers>
      <Layer>
        <FontString text="Status unset" parentKey="StatusText" inherits="GameFontHighlight">
          <Anchors>
            <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" y="15" x="-20"/>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
  </Frame>
</Ui>
