<Ui xmlns="http://www.blizzard.com/wow/ui/">
  <Frame
    name="JournalatorSaleRatesDisplayTemplate"
    virtual="true"
    mixin="JournalatorDisplayTabMixin"
  >
    <Scripts>
      <OnLoad method="OnLoad" />
      <OnUpdate method="OnUpdate" />
    </Scripts>

    <Frames>
      <Button inherits="RefreshButtonTemplate">
        <Anchors>
            <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeKey="$parent" x="-20" y="25"/>
        </Anchors>
        <Scripts>
          <OnClick>
            self:GetParent():RefreshButtonClicked()
          </OnClick>
        </Scripts>
      </Button>

      <EditBox parentKey="SearchFilter" autoFocus="false" historyLines="1" bytes="64" inherits="SearchBoxTemplate">
        <Size x="250" y="22"/>
        <Anchors>
          <Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeKey="$parent" x="10" y="22"/>
        </Anchors>
      </EditBox>

      <Frame parentKey="ResultsListing" inherits="AuctionatorResultsListingTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT"/>
          <Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="-20" y="20"/>
        </Anchors>
      </Frame>

      <Frame parentKey="ResultsListingInset" inherits="AuctionatorInsetDarkTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent.ResultsListing" relativePoint="TOPLEFT" x="-5" y="-24"/>
          <Anchor point="BOTTOMRIGHT" relativeKey="$parent.ResultsListing" relativePoint="BOTTOMRIGHT" y="-2"/>
        </Anchors>
      </Frame>

      <Frame parentKey="DataProvider" mixin="JournalatorSaleRatesDataProviderMixin">
        <Scripts>
          <OnLoad method="OnLoad"/>
          <OnShow method="OnShow"/>
          <OnUpdate method="OnUpdate"/>
        </Scripts>
      </Frame>
    </Frames>
  </Frame>
</Ui>
